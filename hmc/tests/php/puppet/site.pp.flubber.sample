# User configuration
$mapred_user = "hrt_mr"
$hdfs_user = "hrt_hdfs"

$io_sort_spill_percent = 0.8

# Hadoop configuration
$dfs_data_dir = "/grid/0/hdp/hdfs/data,/grid/1/hdp/hdfs/data,/grid/2/hdp/hdfs/data,/grid/3/hdp/hdfs/data"
